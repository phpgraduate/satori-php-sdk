{
    "name": "phpgraduate/satoriPHPSDK",
    "type": "library",
    "description": "Satori PHP SDK",
    "keywords": ["Satori", "PHP SDK"],
    "homepage": "https://github.com/phpgraduate/satoriPHPSDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavan Gudhe",
            "email": "pavangudhe@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "react/socket": "0.4.* | 0.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "cboden/ratchet": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "satoriPHPSDK\\": "src/satoriPHPSDK"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "satoriPHPSDK\\Tests\\": ["tests/satoriPHPSDK/Tests", "tests/satoriPHPSDK/Tests/_includes"]
        }
    }
}